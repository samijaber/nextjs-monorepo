{
  "name": "@samijaber/isolated-vm-test",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "bunchee --runtime=node",
    "pkg:attw": "attw --pack .",
    "pkg:publint": "publint --strict",
    "lint-pkg": "pnpm pkg:publint && pnpm pkg:attw",
    "publish": "pnpm publish --tag next --access public",
    "release": "pnpm build && pnpm publish"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.13.3",
    "@types/node": "^20",
    "bunchee": "^4.1.1",
    "publint": "^0.2.6",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "isolated-vm": "^4.6.0"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "files": [
    "dist",
    "init"
  ],
  "main": "./dist/es/index.mjs",
  "module": "./dist/es/index.mjs",
  "exports": {
    "./init": {
      "import": "./init/index.mjs",
      "require": "./init/index.cjs"
    },
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  }
}
